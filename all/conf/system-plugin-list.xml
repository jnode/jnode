<?xml version="1.0" encoding="UTF-8"?>
<!--
System Plugin List

These (system) plugins are always included in the init-jar.
-->
<plugin-list name="system">

    <plugin id="org.apache.jakarta.log4j"/>
    <plugin id="nanoxml"/>
    <plugin id="org.classpath.core"/>

    <plugin id="org.jnode.runtime"/>
    <plugin id="org.jnode.runtime.core"/>
    <plugin id="org.jnode.security"/>
    <plugin id="org.jnode.util"/>
    <plugin id="org.jnode.vm"/>
    <plugin id="org.jnode.vm.core"/>


            
    <!--
     Plugins needed for setting up
     the minimal filesystem environment
     required by java core librarries.
    -->
    <plugin id="org.jnode.work"/>
    <plugin id="org.jnode.driver"/>
    <plugin id="org.jnode.driver.block"/>
    <plugin id="org.jnode.partitions"/>
    <plugin id="org.jnode.fs"/>
    <plugin id="org.jnode.fs.service"/>
    <plugin id="org.jnode.fs.ramfs"/>
    <plugin id="org.jnode.fs.ramfs.def"/>

</plugin-list>
