<!DOCTYPE plugin SYSTEM "jnode.dtd">

<plugin id="net.sf.cglib" 
        name="cglib classes"
        version="2.1"
        provider-name="cglib.sf.net"
        license-name="apache2.0">
        
  <runtime>
    <library name="cglib.jar">
      <export name="net.sf.cglib.*"/>
      <export name="net.sf.cglib.core.*"/>
      <export name="net.sf.cglib.proxy.*"/>
      <export name="net.sf.cglib.reflect.*"/>
    </library>
  </runtime>        
</plugin>
