<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin SYSTEM "jnode.dtd">

<fragment id="org.classpath.ext.sql"
  name="Classpath javax.sql classes"
  version="@VERSION@"
  plugin-id="rt"
  plugin-version="@VERSION@"
  provider-name="Classpath"
  provider-url="http://classpath.org"
  license-name="classpath">

  <runtime>
    <library name="classlib.jar">
      <export name="java.sql.*"/>
        
      <export name="javax.sql.*"/>
      <export name="javax.sql.rowset.*"/>
      <export name="javax.sql.rowset.serial.*"/>
      <export name="javax.sql.rowset.spi.*"/>
      
      <export name="javax.transaction.*"/>
      <export name="javax.transaction.xa.*"/>
    </library>
  </runtime>
  
</fragment>
