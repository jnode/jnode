    <alias name="xjc" class="com.sun.tools.internal.xjc.Driver"/>
    <alias name="schemagen" class="com.sun.tools.internal.jxc.SchemaGenerator"/>
    <alias name="wsgen" class="com.sun.tools.internal.ws.WsGen"/>
