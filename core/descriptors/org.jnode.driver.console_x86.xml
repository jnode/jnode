<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin SYSTEM "jnode.dtd">

<plugin id="org.jnode.driver.console" 
        name="JNode X86 Console driver"
        version="@VERSION@"
        provider-name="JNode.org"
        class="org.jnode.driver.console.x86.ConsoleManager">
        
  <requires>
    <import plugin="org.jnode.driver.console.core"/>
  </requires>
        
  <runtime>
    <library name="jnode-core.jar">
      <export name="org.jnode.driver.console.x86.*"/>
    </library>
  </runtime>
        
</plugin>