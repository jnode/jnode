<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin SYSTEM "jnode.dtd">

<plugin id="org.jnode.driver.textscreen" 
        name="JNode X86 Console TextScreen driver for X86"
        version="@VERSION@"
        provider-name="JNode.org"
        license-name="lgpl"
        class="org.jnode.driver.textscreen.x86.PcTextScreenPlugin">
        
  <requires>
    <import plugin="org.jnode.driver.textscreen.core"/>
  </requires>
        
  <runtime>
    <library name="jnode-core.jar">
      <export name="org.jnode.driver.textscreen.x86.*"/>
    </library>
  </runtime>
        
</plugin>