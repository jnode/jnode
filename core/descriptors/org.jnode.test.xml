<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin SYSTEM "jnode.dtd">

<plugin id="org.jnode.test" 
        name="JNode Test classes"
        version="@VERSION@"
        provider-name="JNode.org">
        
  <requires>
    <import plugin="org.jnode.runtime"/>
  </requires>
        
  <runtime>
    <library name="jnode-core.jar">
      <export name="org.jnode.test.*"/>
      <export name="org.jnode.test.bugs.*"/>
    </library>
  </runtime>
        
  <extension point="org.jnode.shell.aliases">
    <alias name="AWTTest"      class="org.jnode.test.AWTTest"/>
    <alias name="URLTest"      class="org.jnode.test.URLTest"/>
    <alias name="TCPTest"      class="org.jnode.test.TCPTest"/>
  </extension>

</plugin>
