<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin SYSTEM "jnode.dtd">

<plugin id="org.jnode.fs.jifs.example" 
        name="Jnode Information FileSystem Extension Example"
        version="@VERSION@"
        license-name="lgpl"
        provider-name="JNode.org">
        
        
  <requires>
  	<import plugin="org.jnode.fs.jifs"/>
	<import plugin="org.jnode.fs.jifs.def"/>
  </requires>
        
  <extension point="org.jnode.fs.jifs.def.info">
    <info class="org.jnode.fs.jifs.JIFSDirectory" name="testdir"/>
    <info class="org.jnode.fs.jifs.JIFSFile" name="testfile"/>
  </extension>
          
</plugin>