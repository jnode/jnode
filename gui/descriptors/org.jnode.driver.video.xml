<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin SYSTEM "jnode.dtd">

<plugin id="org.jnode.driver.video" 
        name="JNode Video interfaces"
        version="@VERSION@"
        license-name="lgpl"
        provider-name="JNode.org">
        
  <requires>
    <import plugin="org.jnode.driver"/>
  </requires>
        
  <runtime>
    <library name="jnode-gui.jar">
      <export name="org.jnode.driver.video.*"/>
      <export name="org.jnode.driver.video.ddc.*"/>
      <export name="org.jnode.driver.video.spi.*"/>
      <export name="org.jnode.driver.video.util.*"/>
    </library>
  </runtime>

</plugin>