<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin SYSTEM "jnode.dtd">

<plugin id="org.jnode.test.gui" 
        name="JNode GUI Test classes"
        version="@VERSION@"
        provider-name="JNode.org">
        
  <requires>
    <import plugin="org.jnode.runtime"/>
  </requires>
        
  <runtime>
    <library name="jnode-gui.jar">
      <export name="org.jnode.test.gui.*"/>
    </library>
  </runtime>
        
  <extension point="org.jnode.shell.aliases">
    <alias name="AWTTest"      class="org.jnode.test.gui.AWTTest"/>
    <alias name="WTTest"       class="org.jnode.test.gui.JNodeWTTest"/>
  </extension>

</plugin>
