<testSet title="POSIX command tests">
    <plugin id="org.jnode.shell.command.posix"/>
    <testSpec title="true command" command="org.jnode.shell.command.posix.TrueCommand" 
              runMode="AS_ALIAS" rc="0"/>
    <testSpec title="false command" command="org.jnode.shell.command.posix.FalseCommand"
              runMode="AS_ALIAS" rc="1"/>
    <testSpec title="expr 1 + 1" command="org.jnode.shell.command.posix.ExprCommand"
              runMode="AS_ALIAS" rc="0">
        <arg>1</arg>
        <arg>+</arg>
        <arg>1</arg>
        <output>2
</output>
    </testSpec>
</testSet>